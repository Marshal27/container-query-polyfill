{
  "name": "shadow-container-query-polyfill",
  "version": "1.1.3",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marshal27/shadow-container-query-polyfill"
  },
  "module": "dist/shadow-container-query-polyfill.module.js",
  "unpkg": "dist/shadow-container-query-polyfill.modern.js",
  "publishConfig": {
    "source": "src/index.ts"
  },
  "exports": {
    "default": "./dist/shadow-container-query-polyfill.module.js"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/*.js",
    "package.json"
  ],
  "scripts": {
    "build:clean": "rimraf ./dist/",
    "build:wpt": "microbundle -f modern --output dist/shadow-container-query-polyfill.js --no-compress --define PACKAGE_VERSION=${npm_package_version},IS_WPT_BUILD=1",
    "build:modern": "microbundle -f modern --output dist/shadow-container-query-polyfill.js --no-sourcemap --define PACKAGE_VERSION=${npm_package_version},IS_WPT_BUILD=0",
    "build:esm": "microbundle -f esm --output dist/shadow-container-query-polyfill.js --no-sourcemap --define PACKAGE_VERSION=${npm_package_version},IS_WPT_BUILD=0",
    "lint": "eslint '**/*.ts'",
    "prettier:fix": "prettier --write .",
    "serve": "superstatic -p 9606 .",
    "test": "node --loader ts-node/esm ./tests/wpt.ts"
  },
  "contributors": [],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/async": "^3.2.14",
    "@types/selenium-webdriver": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "async": "^3.2.4",
    "browserstack-local": "^1.5.1",
    "eslint": "^8.12.0",
    "microbundle": "^0.15.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "selenium-webdriver": "^4.3.0",
    "superstatic": "^8.0.0",
    "ts-node": "^10.9.1"
  }
}
